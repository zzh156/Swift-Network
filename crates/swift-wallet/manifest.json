{
    "manifest_version": 3,
    "name": "Swift Wallet",
    "description": "A Web3 wallet for Swift Network",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "public/icons/icon16.png",
        "48": "public/icons/icon48.png",
        "128": "public/icons/icon128.png"
      }
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "src/background/index.ts"
    }
  }